;------------------------------------------------------------------------------
; Literal values test. There is no valid syntax that allows these to appear
; out of the context of an expression.
;
;   literal_value
;       = LITERAL_FLOAT
;       / LITERAL_UNSIGNED
;       / LITERAL_SIGNED
;       / LITERAL_BOOL
;
;------------------------------------------------------------------------------


float some_name = 0.0
float some_name = 1.0
float some_name = 1.1
float some_name = 123123123123123123.0
float some_name = 0.123123123123123123
; the '-' is a unary minus in the expression
float some_name = -1.0e12
float some_name = 10e-2
float some_name = 10.1e-12
float some_name = 10e2
float some_name = 10.1e12

